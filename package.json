  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "format": "prettier --list-different '**/*.{ts,tsx,json,md, gql}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,json,md,gql}'",
    "tslint": "tslint --project . --format stylish",
    "tslint:fix": "tslint --fix --project . --format stylish",
    "sasslint": "sass-lint -c .sass-lint.yml -v -q",
    "lint": "yarn format && yarn tslint && yarn sasslint",
    "lint:fix": "yarn format:fix && yarn tslint:fix && yarn sasslint",
    "test:unit": "mocha --require ts-node/register --require jsdom-global/register --require ./test/setupTests.ts 'test/**/*.spec.ts?(x)'",
    "test:unit-coverage": "nyc mocha --require ts-node/register --require jsdom-global/register --require ./test/setupTests.ts 'test/**/*.spec.ts?(x)'",
    "test": "yarn lint && yarn test:unit",
    "test:coverage": "yarn lint && yarn test:unit-coverage",
    "codecov": "codecov"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta.3",
