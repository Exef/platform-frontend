  "devDependencies": {
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/reactstrap": "^5.0.10",
    "awesome-typescript-loader": "^3.4.1",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.10.1",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1",
    "webpack-hot-middleware": "^2.21.0",
    "@types/chai": "^4.1.0",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^2.2.46",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^4.1.0",
    "sass-lint": "^1.12.1",
    "ts-mocha": "^1.1.0",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^4.1.1",
    "typestrict": "^0.0.6"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "format": "prettier --list-different '**/*.{ts,tsx,json,md, gql}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,json,md,gql}'",
    "tslint": "tslint --project . --format stylish",
    "tslint:fix": "tslint --fix --project . --format stylish",
    "sasslint": "sass-lint -c .sass-lint.yml -v -q",
    "lint": "yarn format && yarn tslint && yarn sasslint",
    "lint:fix": "yarn format:fix && yarn tslint:fix && yarn sasslint",
    "test":
      "yarn lint && ts-mocha --require ./test/setupTests.ts 'test/**/*.spec.ts?(x)'"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reactstrap": "^5.0.0-alpha.4"
  }
}
